{
  "id": "book",
  "type": "SharedSlice",
  "name": "Books 3D",
  "description": "A 3D book showcase component",
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "docURL": "...",
      "version": "initial",
      "description": "Default",
      "imageUrl": "",
      "primary": {
        "title": {
          "type": "StructuredText",
          "config": {
            "label": "Title",
            "placeholder": "Section Title",
            "allowTargetBlank": true,
            "single": "heading2"
          }
        },
        "description": {
          "type": "StructuredText",
          "config": {
            "label": "Description",
            "placeholder": "A description of the book collection",
            "allowTargetBlank": true,
            "multi": "paragraph,strong,em"
          }
        }
      },
      "items": {
        "bookTitle": {
          "type": "Text",
          "config": {
            "label": "Book Title",
            "placeholder": "Enter book title"
          }
        },
        "bookCover": {
          "type": "Image",
          "config": {
            "label": "Book Cover",
            "constraint": {
              "width": 800,
              "height": 1200
            },
            "thumbnails": []
          }
        }
      }
    }
  ]
}